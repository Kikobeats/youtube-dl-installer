{
  "name": "youtube-dl-installer",
  "description": "Platform independent binary installer of youtube-dl for node projects",
  "homepage": "https://documentup.com/Kikobeats/youtube-dl-installer",
  "version": "1.0.0",
  "main": "index.js",
  "author": {
    "email": "josefrancisco.verdu@gmail.com",
    "name": "Kiko Beats",
    "url": "https://kikobeats.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kikobeats/youtube-dl-installer.git"
  },
  "bugs": {
    "url": "https://github.com/Kikobeats/youtube-dl-installer/issues"
  },
  "keywords": [
    "binary",
    "download",
    "youtube-dl"
  ],
  "dependencies": {
    "download": "~7.0.0",
    "make-dir": "~1.3.0",
    "reachable-url": "~1.0.1"
  },
  "devDependencies": {
    "finepack": "latest",
    "git-dirty": "latest",
    "husky": "latest",
    "lint-staged": "latest",
    "prettier-standard": "latest",
    "standard": "latest",
    "standard-markdown": "latest"
  },
  "engines": {
    "node": ">= 8"
  },
  "files": [
    "bin",
    "index.js",
    "scripts"
  ],
  "scripts": {
    "clean": "rm -rf node_modules",
    "lint": "standard-markdown && standard",
    "postinstall": "./scripts/download",
    "precommit": "lint-staged",
    "pretest": "npm run lint",
    "pretty": "prettier-standard index.js {core,test,bin}/**/*.js --single-quote",
    "test": "exit 0"
  },
  "license": "MIT",
  "lint-staged": {
    "package.json": [
      "finepack",
      "git add"
    ],
    "*.js": [
      "prettier-standard",
      "git add"
    ],
    "*.md": [
      "standard-markdown",
      "git add"
    ]
  }
}
